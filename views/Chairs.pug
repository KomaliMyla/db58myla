extends layout

block content
  h1= title
  - let images = results
    table(class="table table-bordered")
        thead
            tr 
                th(scope="col") Chair_type 
                th(scope="col") Color 
                th(scope="col") Cost
        tbody        
            - for(let i=0;i<images.length;i++){
                tr
                    td  #{images[i].Chair_type}
                    td  #{images[i].Color}
                    td  #{images[i].Cost}
            - }